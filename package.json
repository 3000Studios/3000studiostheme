  "name": "3000studios",
  "version": "1.23",
  "description": "3000 Studios WordPress Theme tooling",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "check": "npm run lint",
    "prepare": "husky",
    "watch": "bash scripts/auto-update-watcher.sh",
    "watch:commit": "AUTO_COMMIT=true bash scripts/auto-update-watcher.sh",
    "watch:push": "AUTO_PUSH=true bash scripts/auto-update-watcher.sh",
    "refresh-server": "node scripts/refresh-server.js",
    "dev": "concurrently \"npm run watch\" \"npm run refresh-server\"",
    "version:bump": "node scripts/version-bump.js",
    "workflows:run": "bash scripts/trigger-all-workflows.sh",
    "workflows:list": "gh workflow list --repo 3000Studios/3000studiostheme",
    "workflows:status": "gh run list --repo 3000Studios/3000studiostheme --limit 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3000Studios/3000studiostheme.git"
  },
  "keywords": [],
  "author": "3000 Studios",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/3000Studios/3000studiostheme/issues"
  },
  "homepage": "https://github.com/3000Studios/3000studiostheme#readme",
  "dependencies": {
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "openai": "^6.7.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "eslint": "^9.39.0",
    "husky": "^9.1.7",
    "concurrently": "^9.1.2"
  }
}
